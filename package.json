{
    "name": "mobx-react-devtools",
    "version": "4.2.10",
    "description": "Dev-tools for MobX and React",
    "main": "index.js",
    "typings": "index",
    "repository": {
        "type": "git",
        "url": "https://github.com/mobxjs/mobx-react-devtools.git"
    },
    "scripts": {
        "build": "webpack",
        "prepublish": "npm run build",
        "start-todomvc": "cross-env NODE_ENV=development webpack-dev-server --config webpack.todomvc.config.js --hot --inline",
        "start-chrome": "cross-env NODE_ENV=development webpack --watch --config webpack.chrome.config.js",
        "test": "mocha 'src/**/*-test.js' --compilers js:babel-core/register",
        "lint": "eslint 'src/**/*.js' 'src/**/*.jsx' && echo 'JS OK' ; exit 0"
    },
    "author": "Michel Weststrate",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mobxjs/mobx/issues"
    },
    "homepage": "https://mobxjs.github.io/mobx",
    "peerDependencies": {
        "mobx": "^2.2.0",
        "mobx-react": "^3.3.0 || ^4.0.0"
    },
    "devDependencies": {
        "and": "0.0.0",
        "babel": "^6.5.1",
        "babel-core": "^6.5.1",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^6.2.2",
        "babel-plugin-transform-class-properties": "^6.5.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-preset-es2015": "^6.5.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-1": "^6.22.0",
        "cross-env": "^3.1.4",
        "director": "^1.2.8",
        "es6-object-assign": "^1.0.1",
        "eslint": "^3.15.0",
        "eslint-config-airbnb": "^14.1.0",
        "eslint-loader": "^1.6.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^4.0.0",
        "eslint-plugin-react": "^6.9.0",
        "file-loader": "^0.8.5",
        "jsdom": "^9.10.0",
        "mobx": "^3.1.5",
        "mobx-react": "file:mobx-react",
        "react": "^15.4.2",
        "react-addons-test-utils": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-test-renderer": "^15.4.2",
        "sinon": "^1.17.7",
        "todomvc-common": "^1.0.3",
        "ts-loader": "^2.0.1",
        "typescript": "^2.2.1",
        "unexpected": "^10.25.0",
        "unexpected-react": "^3.4.0",
        "unexpected-sinon": "^10.6.1",
        "url-loader": "^0.5.7",
        "webpack": "^1.12.0"
    },
    "keywords": [
        "mobx",
        "mobservable",
        "react-component",
        "react",
        "reactjs",
        "reactive",
        "devtools"
    ]
}
